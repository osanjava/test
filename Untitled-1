areaName




const allTrading = util.session.get('allTrading');
const allTradingCode=util.session.get('allTradingCode');
if (allTrading.length > 0) {
var arr = [];
allTrading.map((item, index)=>{
    arr.push(Object.assign({'value': item.areaCode}, {'label': item.areaName}))
})
bindUpdate(store, 'selectFluctuationOfConsumption.options', arr);
if (arr.length > 3) {
    bindUpdate(store, 'selectFluctuationOfConsumption.value', [arr[0].value, arr[1].value, arr[2].value]);
} else if (arr.length > 2) {
    bindUpdate(store, 'selectFluctuationOfConsumption.value', [arr[0].value, arr[1].value]);
} else if (arr.length > 1) {
    bindUpdate(store, 'selectFluctuationOfConsumption.value', [arr[0].value]);
}
}

const allTown = util.session.get('allTown');
const allTownCode = util.session.get('allTownCode');
if (allTown.length > 0) {
var arr = [];
allTown.map((item, index)=>{
    arr.push(Object.assign({'value': item.areaCode}, {'label': item.areaName}))
})
bindUpdate(store, 'selectFluctuationOfConsumption.options', arr);
if (arr.length > 3) {
    bindUpdate(store, 'selectFluctuationOfConsumption.value', [arr[0].value, arr[1].value, arr[2].value]);
} else if (arr.length > 2) {
    bindUpdate(store, 'selectFluctuationOfConsumption.value', [arr[0].value, arr[1].value]);
} else if (arr.length > 1) {
    bindUpdate(store, 'selectFluctuationOfConsumption.value', [arr[0].value]);
}
}

